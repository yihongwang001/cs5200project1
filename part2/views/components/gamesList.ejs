<h2>Olympic Games</h2>
<div class="row g-3">
  <% for (let ref of games) { %>

    <div class="col-4">
      <div class="card p-1">
        <h5 class="card-title"><%= ref.city %></h5>
        <div class="card-text">
          <div><%= ref.year %> Year</div>
          <div><%= ref.season %> Season</div>
          <div><%= ref.city %> City</div>
          <div>
          </div>
        </div>
      </div>
    </div>


  <% } %>

 <%- include("./gamesPagination.ejs", { currentPage, lastPage }) %>

</div>
